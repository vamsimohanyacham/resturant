#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/bin/esvalidate.js/node_modules:/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/bin/node_modules:/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/node_modules:/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules/.pnpm/esprima@1.2.2/node_modules:/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules:/home/nxtwave/reactjs/coding-practices/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules/.pnpm/jsonpath@1.1.1/node_modules/jsonpath/node_modules:/home/nxtwave/reactjs/coding-practices/enhancementOfRestaurantApp/node_modules/.pnpm/jsonpath@1.1.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@1.2.2/node_modules/esprima/bin/esvalidate.js" "$@"
else 
  exec node  "$basedir/../../../../../esprima@1.2.2/node_modules/esprima/bin/esvalidate.js" "$@"
fi
